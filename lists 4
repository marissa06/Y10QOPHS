# challenge 4 - lists

show1 = "friends"
show2 = "erased"
show3 = "aot"
show4 = "vd"
tvShows = [show1,show2,show3,show4]
print(show1)
print(show2)
print(show3)
print(show4)
show5= input("enter a your favourite tv programme>>>")
location = int(input("what position do you want it in the list(0-4)>>>"))
tvShows.insert(location,show5)
print(tvShows)
